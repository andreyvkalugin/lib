<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>NewRecordInLibraryServlet</servlet-name>
    <servlet-class>ru.kalugin.elibrary.NewRecordInLibraryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewRecordInLibraryServlet</servlet-name>
    <url-pattern>/NewRecordInLibraryServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LibraryBooks</servlet-name>
    <jsp-file>/LibraryBooks.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>LibraryBooks</servlet-name>
    <url-pattern>/LibraryBooks</url-pattern>
  </servlet-mapping>
</web-app>

